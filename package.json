    "monaco-editor": "^0.52.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9",
    "zod": "^3.25.76"
