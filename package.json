    "eslint-plugin-vue": "~10.5.0",
    "fast-check": "^4.3.0",
    "jiti": "^2.6.1",
    "msw": "^2.12.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^4.0.10",
    "vue-tsc": "^3.1.1"
