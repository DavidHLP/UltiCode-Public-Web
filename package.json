{
  "name": "edit-markdown",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "validate:mocks": "node scripts/validate-mock-data.mjs",
    "validate:mocks:verbose": "node scripts/validate-mock-data.mjs --verbose",
    "validate:mocks:strict": "node scripts/validate-mock-data.mjs --strict"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.6.1",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/vue-table": "^8.21.3",
    "@unovis/ts": "^1.6.1",
    "@unovis/vue": "^1.6.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.6.0",
    "highlight.js": "^11.9.0",
    "katex": "^0.16.10",
    "lucide-vue-next": "^0.552.0",
    "markdown-it": "^14.1.0",
